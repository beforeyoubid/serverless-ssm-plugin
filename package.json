{
  "name": "serverless-ssm-plugin",
  "version": "1.1.0",
  "description": "Serverless plugin to add secrets manager secrets to function bundles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Bailey <bailey@beforeyoubid.com.au>",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "2.1251.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:beforeyoubid/serverless-ssm-plugin.git"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint '**/*.{ts,tsx,js,jsx}' --quiet",
    "ts": "tsc --noEmit",
    "build-and-publish": "npm run build && npm publish --access public",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/*.ts --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@types/jest": "29.2.2",
    "@types/lodash": "4.14.188",
    "@types/moment": "^2.13.0",
    "@types/node": "18.11.9",
    "@types/serverless": "3.12.8",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "asl-types": "^1.2.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "jest-junit": "14.0.1",
    "pre-commit": "^1.1.3",
    "prettier": "2.7.1",
    "serverless": "3.24.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "secrets manager"
  ],
  "files": [
    "dist/**/*"
  ]
}
